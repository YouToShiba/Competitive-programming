   "source": [
    "# 解答\n",
    "N = int(input())\n",
    "count = 0\n",
    "pt, px, py = 0, 0, 0\n",
    "for i in range(N):\n",
    "    t, x, y = map(int, input().split())\n",
    "    if abs(x-px)+abs(y-py) <= t-pt and t % 2 == (x+y) % 2:    # abs:絶対値\n",
    "        count += 1\n",
    "    pt, px, py = t, x, y\n",
    "print(\"Yes\" if count == N else \"No\")"
   ]
